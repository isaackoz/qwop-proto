{
  "compilerOptions": {
    "target": "ESNext",
    "esModuleInterop": true,
    "allowJs": true,
    "baseUrl": ".",
    "moduleDetection": "force",
    "isolatedModules": true,
    "composite": true,
    "skipLibCheck": true,
    "tsBuildInfoFile": "./buildcache/.tsbuildinfo",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "noEmit": true
  },
  "include": ["scripts/**/*.ts"]
}
